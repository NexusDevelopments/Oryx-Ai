{
  "rewrites": [
    { "source": "/", "destination": "/public/index.html" },
    { "source": "/styles.css", "destination": "/public/styles.css" },
    { "source": "/main.js", "destination": "/public/main.js" },
    { "source": "/oryx-logo.svg", "destination": "/public/oryx-logo.svg" },
    { "source": "/oryx-logo-text.svg", "destination": "/public/oryx-logo-text.svg" },
    { "source": "/(.*)", "destination": "/public/$1" }
  ]
}
